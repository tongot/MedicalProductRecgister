@model IEnumerable<Pharmacy.Models.RegisterViewModel>
@{
    ViewBag.Title = "GetAllUsers";
}
<h2>Users</h2>
@Html.ActionLink("Create", "Register", "Account", routeValues: null, htmlAttributes: new { @class = "btn btn-primary" })
<table class="table">
    <thead>
        <tr>
            <th>
                <label class="col-form-label">Name</label>
            </th>
            <th>
                <label class="col-form-label">Surname</label>
            </th>
            <th>
                <label class="col-form-label">National Id</label>
            </th>
            <th>
                <label class="col-form-label">Email</label>
            </th>
        </tr>
    </thead>
    <tbody>
        @{
            foreach (var item in Model)
            {
                <tr>
                    <td>
                        @item.Name
                    </td>
                    <td>
                        @item.Surname
                    </td>
                    <td>
                        @item.NationalId
                    </td>
                    <td>
                        @item.Email
                    </td>
                    <td>
                        @Html.ActionLink("Details", "Details", "Account", new { id = item.id }, htmlAttributes: new { @class = "btn btn-info btn-sm" })
                    </td>
                </tr>
            }
        }

    </tbody>
</table>